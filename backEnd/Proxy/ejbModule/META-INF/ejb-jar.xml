<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:ejb="http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
	version="3.1">
	<display-name>Proxy</display-name>
	<enterprise-beans>
		<session>
			<ejb-name>BarnyardProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.BarnyardBean</ejb-class>
		</session>
		<session>
			<ejb-name>BarnyardCapacityProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.BarnyardCapacityBean</ejb-class>
		</session>
		<session>
			<ejb-name>CattleClassProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.CattleClassBean</ejb-class>
		</session>
		<session>
			<ejb-name>CattleTypeProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.CattleTypeBean</ejb-class>
		</session>
		<session>
			<ejb-name>EnterpriseContactProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.EnterpriseContactBean</ejb-class>
		</session>
		<session>
			<ejb-name>RancherProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.RancherBean</ejb-class>
		</session>
		<session>
			<ejb-name>EnterpriseRancherProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.EnterpriseRancherBean</ejb-class>
		</session>
		<session>
			<ejb-name>RancherInvoiceProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.RancherInvoiceBean</ejb-class>
		</session>
		<session>
			<ejb-name>RancherContactProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.RancherContactBean</ejb-class>
		</session>
		<session>
			<ejb-name>MeasurementUnitProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.MeasurementUnitBean</ejb-class>
		</session>
		<session>
			<ejb-name>MeasurementUnitEquivalenceProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.MeasurementUnitEquivalenceBean</ejb-class>
		</session>
		<session>
			<ejb-name>ReceptionProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.ReceptionBean</ejb-class>
		</session>
		<session>
			<ejb-name>ReceptionBarnyardProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.ReceptionBarnyardBean</ejb-class>
		</session>
		<session>
			<ejb-name>ReceptionHeadcountProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.ReceptionHeadcountBean</ejb-class>
		</session>
		<session>
			<ejb-name>FeedOrderProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.FeedOrderBean</ejb-class>
		</session>
		<session>
			<ejb-name>FoodProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.FoodBean</ejb-class>
		</session>
		<session>
			<ejb-name>FeedOrderDetailsProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.FeedOrderDetailsBean</ejb-class>
		</session>
		<session>
			<ejb-name>InspectionProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.InspectionBean</ejb-class>
		</session>
		<session>
			<ejb-name>InspectionBarnyardProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.InspectionBarnyardBean</ejb-class>
		</session>
		<session>
			<ejb-name>InspectionCodeProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.InspectionCodeBean</ejb-class>
		</session>
		<session>
			<ejb-name>FeedOrderBarnyardProxy</ejb-name>
			<ejb-class>com.tramex.sisoprega.proxy.bean.FeedOrderBarnyardBean</ejb-class>
		</session>
	</enterprise-beans>
	<assembly-descriptor>
		<security-role>
			<role-name>mx_usr</role-name>
		</security-role>
		<security-role>
			<role-name>sisoprega_admin</role-name>
		</security-role>
		<method-permission>
			<role-name>mx_usr</role-name>
			<method>
				<ejb-name>BarnyardProxy</ejb-name>
				<method-name>Read</method-name>
			</method>
			<method>
				<ejb-name>BarnyardCapacityProxy</ejb-name>
				<method-name>Read</method-name>
			</method>
			<method>
				<ejb-name>CattleClassProxy</ejb-name>
				<method-name>Read</method-name>
			</method>
			<method>
				<ejb-name>CattleTypeProxy</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>EnterpriseContactProxy</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>EnterpriseRancherProxy</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>FeedOrderProxy</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>FeedOrderBarnyardProxy</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>RancherProxy</ejb-name>
				<method-name>*</method-name>
			</method>
		</method-permission>
		<method-permission>
			<role-name>sisoprega_admin</role-name>
			<method>
				<ejb-name>BarnyardProxy</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>BarnyardCapacityProxy</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>CattleClassProxy</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>CattleTypeProxy</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>EnterpriseContactProxy</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>FeedOrderProxy</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>FeedOrderBarnyardProxy</ejb-name>
				<method-name>*</method-name>
			</method>
		</method-permission>
	</assembly-descriptor>
</ejb-jar>