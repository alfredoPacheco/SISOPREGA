<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:ejb="http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd" version="3.1">
  <display-name>Proxy</display-name>
  <enterprise-beans>
    <session>
      <ejb-name>RancherProxy</ejb-name>
      <ejb-class>com.tramex.sisoprega.proxy.bean.Rancher</ejb-class>
    </session>
    <session>
      <ejb-name>EnterpriseRancherProxy</ejb-name>
      <ejb-class>com.tramex.sisoprega.proxy.bean.EnterpriseRancher</ejb-class>
    </session>
    <session>
      <ejb-name>RancherInvoiceProxy</ejb-name>
      <ejb-class>com.tramex.sisoprega.proxy.bean.RancherInvoice</ejb-class>
    </session>
    <session>
      <ejb-name>RancherContactProxy</ejb-name>
      <ejb-class>com.tramex.sisoprega.proxy.bean.RancherContact</ejb-class>
    </session>
    <session>
      <ejb-name>EnterpriseContactProxy</ejb-name>
      <ejb-class>com.tramex.sisoprega.proxy.bean.EnterpriseContact</ejb-class>
    </session>
    <session>
      <ejb-name>CattleClassProxy</ejb-name>
      <ejb-class>com.tramex.sisoprega.proxy.bean.CattleClassBean</ejb-class>
    </session>
    <session>
      <ejb-name>CattleTypeProxy</ejb-name>
      <ejb-class>com.tramex.sisoprega.proxy.bean.CattleTypeBean</ejb-class>
    </session>
    <session>
    	<ejb-name>BarnyardProxy</ejb-name>
    	<ejb-class>com.tramex.sisoprega.proxy.bean.BarnyardBean</ejb-class>
    </session>
    <session>
    	<ejb-name>BarnyardCapacityProxy</ejb-name>
    	<ejb-class>com.tramex.sisoprega.proxy.bean.BarnyardCapacityBean</ejb-class>
    </session>
    <session>
    	<ejb-name>MeasurementUnitProxy</ejb-name>
    	<ejb-class>com.tramex.sisoprega.proxy.bean.MeasurementUnitBean</ejb-class>
    </session>
    <session>
    	<ejb-name>MeasurementUnitEquivalenceProxy</ejb-name>
    	<ejb-class>com.tramex.sisoprega.proxy.bean.MeasurementUnitEquivalenceBean</ejb-class>
    </session>
  </enterprise-beans>
  <assembly-descriptor>
  	<security-role>
  		<role-name>mx_usr</role-name>
  	</security-role>
  	<method-permission>
  		<role-name>mx_usr</role-name>
  		<method>
  			<ejb-name>RancherProxy</ejb-name>
  			<method-intf>Remote</method-intf>
  			<method-name>Read</method-name>
  		</method>
  	</method-permission>
  </assembly-descriptor>
</ejb-jar>